{
  "extends": "airbnb",
  "rules": {
    "import/no-extraneous-dependencies": [2, {"devDependencies": true}],
    "import/newline-after-import": 0,
    "comma-dangle": [2, "never"],
    "arrow-body-style": 0,
    "jsx-quotes": [2, 'prefer-single'],
    "react/no-unused-prop-types": 0
  },
  "env": {
    "browser": true
  }
}
